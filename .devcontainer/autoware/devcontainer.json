{
    "name": "autoware",
    "image": "ghcr.io/autowarefoundation/autoware:universe-devel-20250204-amd64",
    "remoteUser": "autoware",
    "runArgs": [
      "--cap-add=SYS_PTRACE",
      "--security-opt",
      "seccomp=unconfined",
      "--net=host",
      "--volume=/etc/localtime:/etc/localtime:ro"
    ],
    "customizations": {
      "vscode": {
        "settings.json": {
          "terminal.integrated.profiles.linux": { "bash": { "path": "/bin/bash" } }
        }
      }
    }
  }
  